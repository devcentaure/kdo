{% block navbar %}

    <nav class="top-bar" data-topbar role="navigation">
        <ul class="title-area">
            <li class="name">
                <h1>
                    <a href="#">
                        {{ 'views.navbar.title'|trans }}
                    </a>
                </h1>
            </li>
            <li class="toggle-topbar menu-icon">
                <a href="#"><span>{{ 'views.navbar.menu'|trans }}</span></a>
            </li>
        </ul>
        <section class="top-bar-section">
            <ul class="right">
                <li class="">
                    <a href="#">
                        {{ 'views.navbar.link1'|trans }}
                    </a>
                </li>
                <li class="has-dropdown">
                    <a href="#">
                        {{ 'views.navbar.link2'|trans }}
                    </a>
                    <ul class="dropdown">
                        <li><a href="#">{{ 'views.navbar.link3'|trans }}</a></li>
                        <li class=""><a href="#">
                            {{ 'views.navbar.link3'|trans }}
                        </a></li>
                    </ul>
                </li>
            </ul>
        </section>
    </nav>


{% endblock navbar %}