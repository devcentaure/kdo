            <li class="name">
                <h1>
                    <a href="#">
                        {{ 'views.navbar.title'|trans }}
                    </a>
                </h1>
            </li>
            <li class="toggle-topbar menu-icon">
                <a href="#"><span>{{ 'views.navbar.menu'|trans }}</span></a>
