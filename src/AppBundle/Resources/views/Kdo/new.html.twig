{% extends '::base.html.twig' %}

{% block title %}
    {{ 'app_kdo.index.title'|trans }}
{% endblock title %}


{% block page %}
    <div class="row">
        <div class="medium-12 columns">
            <h1 class="text-center title">Kdo creation</h1>
        </div>
    </div>

    <div class="panel">
        {{ form(form) }}
    </div>

    <div class="row">
        <div class="small-4 columns text-left">
            <a class="link-return" href="{{ path('app_kdo') }}">
                <i class="icon-arrow-left icon-large vertical"></i>
                {{ 'link.return'|trans }}
            </a>
        </div>
    </div>

{% endblock %}
